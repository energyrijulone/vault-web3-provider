    "web3-provider-engine": "^14.0.6",
    "webpack": "^4.0.1",
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "vault-api": "github:zippiehq/vault-api",
    "web3": "^1.0.0-beta.33",
    "webpack-cli": "^2.0.13",
    "wscat": "^2.1.1"
  }
}
